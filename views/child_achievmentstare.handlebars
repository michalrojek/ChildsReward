    <div class="main-panel">
        <nav class="navbar navbar-default navbar-fixed">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navigation-example-2">
                        <span class="sr-only">Włącz nawigację</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="#">STATYSTYKI</a>
                </div>

                <div class="collapse navbar-collapse">
                    
                </div>
            </div>
        </nav>
<div class="content">
            <div class="container-fluid">
                {{#if success_msg}}
                    <div class="alert alert-success">{{success_msg}}</div>
                {{/if}}
                <div class="row">
                    <div class="col-md-12">
                        <div class="card ">
                            <div class="content">
                                <b>Punkty: </b>{{child.score}}
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="card ">
                            <div class="header">
                                <h4 class="title">ODZNACZENIA I NAGRODY</h4>
                            </div>
                            <div class="content">
                                <div class="table-full-width">
                                    <table class="table">
                                        <tbody>
                                            {{#if achievmentsRender}}
                                                {{#each achievmentsRender}}
                                                    <tr>
                                                        <td><b>Tytuł odznaczenia:</b> {{name}}</td>
                                                        <td><b>Opis:</b> {{desc}}</td>

                                                    </tr>
                                                {{/each}}
                                            {{/if}}
                                        </tbody>
                                    </table>
                                </div>

                                <div class="footer">
                                    <hr>
                                    <div class="stats">
                                        
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="card ">
                            <div class="header">
                                <h4 class="title">NAGRODY</h4>
                            </div>
                            <div class="content">
                                <div class="table-full-width">
                                    <table class="table">
                                        <tbody>
                                            {{#if prizesRender}}
                                                {{#each prizesRender}}
                                                    <tr>
                                                        <td><b>Tytuł nagrody:</b> {{name}}</td>
                                                        <td><b>Opis:</b> {{desc}}</td>

                                                    </tr>
                                                {{/each}}
                                            {{/if}}
                                        </tbody>
                                    </table>
                                </div>

                                <div class="footer">
                                    <hr>
                                    <div class="stats">
                                        
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</div>